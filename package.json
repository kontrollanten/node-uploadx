{
  "name": "node-uploadx-src",
  "version": "4.1.7",
  "description": "Node.js resumable upload middleware",
  "keywords": [
    "resumable",
    "upload",
    "express",
    "fastify",
    "middleware",
    "s3",
    "gcloud"
  ],
  "author": {
    "name": "kukhariev",
    "email": "q_h@inbox.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kukhariev/node-uploadx.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.10.0"
  },
  "scripts": {
    "boot": "lerna bootstrap --no-ci",
    "start": "tsnd -r dotenv/config examples/express",
    "prettier": "prettier --write \"**/*.{json,md}\"",
    "build": "rimraf dist && lerna bootstrap && lerna run build",
    "postbuild_": "tsnd scripts/post-build.ts",
    "cleanup": "rimraf upload files dist && lerna clean --yes && lerna run clean",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint \"{packages,examples,test}/**/*.ts\" --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/bytes": "3.1.0",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.8",
    "@types/jest": "26.0.15",
    "@types/multiparty": "0.0.32",
    "@types/node": "10.17.35",
    "@types/node-fetch": "2.5.",
    "@types/rimraf": "3.0.0",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "express": "4.17.1",
    "husky": "4.3.0",
    "jest": "26.6.1",
    "lerna": "^3.22.1",
    "lint-staged": "10.5.0",
    "node-mocks-http": "1.9.0",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "supertest": "6.0.0",
    "ts-jest": "26.4.3",
    "ts-node-dev": "1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.0.5"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{packages,test}/**/*.{ts}": "eslint --fix",
    "*.{md,json}": "prettier --write"
  }
}
